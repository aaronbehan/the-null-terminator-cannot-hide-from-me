#include <stdio.h>

int main(void) {
    char string[] = "The null terminator cannot hide from me!";

    size_t length = sizeof(string) / sizeof(char);

    for (int i = 0; i < length; i++) {

        if (string[i] == '\0') printf("null terminator");  // PROOF OF NULL TERMINATOR

        else printf("%c", string[i]);
    }

}
